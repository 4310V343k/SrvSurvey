<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net7.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWindowsForms>true</UseWindowsForms>
    <ImplicitUsings>enable</ImplicitUsings>
    <StartupObject>SrvSurvey.Program</StartupObject>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="images\planet.ico" />
    <None Remove="images\surface.png" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="images\planet.ico" />
    <Content Include="images\surface.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="FormErrorSubmit.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="FormGroundTarget.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="FormSettings.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="Main.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="PlotBioStatus.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="PlotGrounded.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="PlotPulse.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="PlotTrackTarget.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="SrvSurvey.cs">
      <SubType>Form</SubType>
    </Compile>
    <Compile Update="ViewLogs.cs">
      <SubType>Form</SubType>
    </Compile>
  </ItemGroup>

</Project>